    local Material = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kinlei/MaterialLua/master/Module.lua"))()
    local UI = Material.Load({
        Title = "Demon Slayer: Kaibutsu | Nix Hub Premium",
        Style = 3,
        SizeX = 500,
        SizeY = 400,
        Theme = "Aqua"
   })



   local main = UI.New({
       Title = "Functions"
   })
   local chr = UI.New({
       Title = "Utilities"
   })
  
   
    main.Button({
        Text = "Be Slayer",
        Callback = function()
            -- Script generated by SimpleSpy - credits to exx#9394

            local args = {
                [1] = "Slayer"
            }

        game:GetService("ReplicatedStorage").Remotes.ChangeAlignment:FireServer(unpack(args))
        end,
        Menu = {
            Information = function(self)
                UI.Banner({
                    Text = "Turns you into a slayer!"
                })
            end,
        }   
    })
    

    main.Button({
        Text = "Buy Nichirin",
        Callback = function()
            -- Script generated by SimpleSpy - credits to exx#9394

            local args = {
                [1] = "Nichirin Katana"
            }

            game:GetService("ReplicatedStorage").Remotes.PurchaseItem:FireServer(unpack(args))
        end,
        Menu = {
            Information = function(self)
                UI.Banner({
                    Text = "Buys Nichirin Katana!"
                })
            end,
        }

    })
    

       

   chr.Slider({
       Text = "Walk Speed(Run for it to work!)",
       Callback = function(value)
        game.Players.LocalPlayer.Character.BaseSpeed.Value = value
       end,
       Min = 16,
       Max = 400,
       Def = 16,
       
       
   })


   chr.Slider({
       Text = "Run Speed",
       Callback = function(value)
        game.Players.LocalPlayer.Character.RunSpeed.Value = value
       end,
       Min = 16,
       Max = 400,
       Def = 16
    })


   main.Toggle({
       Text = "Unlock Chat",
       Callback = function(value)
        game:GetService("Players").LocalPlayer.PlayerGui.Chat.Frame.ChatChannelParentFrame.Visible = value
       end,
       Enabled = false,
      
   })


   chr.Toggle({
       Text = "AutoFarm Demons",
       Callback = function(active)
        getgenv().farm = active
        while wait() do
            if not getgenv().farm then break end
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").LivingThings:FindFirstChild("Demon").HumanoidRootPart.CFrame * CFrame.new(0, 0, 3)
            game:GetService("Players").LocalPlayer.Character.Character.Remotes.M1:FireServer()
            if game:GetService("Workspace").LivingThings:FindFirstChild("Demon").Humanoid.Health == 0 then wait(0.01) game:GetService("Workspace").LivingThings:FindFirstChild("Demon"):Destroy() break; end
            wait()
           



        end
        end,
        Enabled = false
        

   })


   chr.Toggle({
    Text = "AutoFarm Demons2(DemonLevel)",
    Callback = function(active)
     getgenv().farm = active
     while wait() do
         if not getgenv().farm then break end
         game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").LivingThings:FindFirstChild("Demon2").HumanoidRootPart.CFrame * CFrame.new(0, 0, 3)
         game:GetService("Players").LocalPlayer.Character.Character.Remotes.M1:FireServer()
         if game:GetService("Workspace").LivingThings:FindFirstChild("Demon").Humanoid.Health == 0 then wait(0.01) game:GetService("Workspace").LivingThings:FindFirstChild("Demon2"):Destroy() break; end
         wait()
        



     end
     end,
     Enabled = false
     

})


chr.Toggle({
    Text = "AutoFarm Demon3(Demon Level)",
    Callback = function(active)
     getgenv().farm = active
     while wait() do
         if not getgenv().farm then break end
         game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").LivingThings:FindFirstChild("Demon3").HumanoidRootPart.CFrame * CFrame.new(0, 0, 3)
         game:GetService("Players").LocalPlayer.Character.Character.Remotes.M1:FireServer()
         if game:GetService("Workspace").LivingThings:FindFirstChild("Demon").Humanoid.Health == 0 then wait(0.01) game:GetService("Workspace").LivingThings:FindFirstChild("Demon3"):Destroy() break; end
         wait()
        



     end
     end,
     Enabled = false
     

})



