     end
     end,
     Enabled = false
     

})

   chr.Button({
       Text = "M2",
       Callback = function()
        game:GetService("Players").LocalPlayer.Character.Character.Remotes.M2:FireServer()
       end,
   })
   chr.Button({
       Text = "Teleport to slayer corps",
       Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").NPCS["Corp Recruiter"].HumanoidRootPart.CFrame
       end,
   })

   main.Button({
    Text = "Rejoin",
    Callback = function()
        local ts = game:GetService("TeleportService")
        local p = game:GetService("Players").LocalPlayer

 

        ts:Teleport(game.PlaceId, p)

        if syn then
            syn.queue_on_teleport("loadstring(game:HttpGet('https://raw.githubusercontent.com/KeySystem-Nix/Scripts/main/kaibutsu'))()")
        end
    end,
})

    chr.Toggle({
        Text = "Auto-Breath",
        Callback = function(v)
            getgenv().breath = v 
            while wait() do
                if not getgenv().breath then break end
                

                game:GetService("Players").LocalPlayer.Character.Character.Remotes.ChargeBreathing:FireServer()
            end
            if getgenv().breath == false then
            game:GetService("Players").LocalPlayer.Character.Character.Remotes.Uncharge:FireServer()
            end
        end,
        Enabled = false
    })


    chr.Dropdown({
        Text = "Trainer Teleports",
        Callback = function(v)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService('Workspace').NPCS[v].HumanoidRootPart.CFrame * CFrame.new(0, 2, 0)
        end,
        Options = {"KyojuroRengoku", "Urokodaki", "Mitsuri", "Corp Recruiter", "Gyomei", "ShinobuKocho", "Jigoro", "SanemiShinazugawa", "MuichiroTokito"},
        Menu = {
            Information = function(self)
                UI.Banner({
                    Text = "Teleports you to the selected trainer!"
                })
            end
        }
    })
